{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../src"
        }
      ],
      "dest": "../ref",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "ref/**.yml",
          "ref/index.md"
        ],
        "src": "../"
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "articles/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "src": "../"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "src": "../"
      }
    ],
    "overwrite": [
      {
        "files": [
          "refdoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ],
        "src": "../"
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "properties": {
      "TargetFramework": "netstandard2.1"
    },
    "globalMetadata": {
      "_appTitle": "GlobalLink Vasont Inspire Models",
      "_appFooter": "Copyright 2020 Transperfect Inc.",
      "_appLogoPath": "images/gl_vasont_logo.png",
      "_appFaviconPath": "images/favicon.ico",
      "_gitContribute": {
        "repo": "https://github.com/vasont-systems/Vasont.Inspire.Models",
        "branch": "master"
      }
    }
  }
}